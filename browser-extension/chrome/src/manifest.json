{
  "manifest_version": 3,
  "name": "TIL",
  "description": "Today I Learned's very own browser extension",
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["icon-128.png", "icon-34.png"],
      "matches": []
    }
  ],
  "permissions": [
    "identity",
    "tabs"
  ]
}
